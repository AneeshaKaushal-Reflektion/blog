#!/usr/bin/env bash

# Safety first!
set -e

run() {
  CMD=$@ && echo && echo ">>> $CMD" && $CMD
}

run $(dirname $0)/build
run cf push pivotal-cf-blog         -f public/prod/manifest.yml    -n pivotal-cf-blog
run cf push pivotal-cf-blog-staging -f public/staging/manifest.yml -n pivotal-cf-blog-staging


